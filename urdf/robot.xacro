            </material>
        </visual>

        <collision>
            <origin rpy="${pi} 0 0" xyz="0 -0.1 0" />
            <geometry>
                <box size="0.03 0.01 0.3" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="${pi} 0 0" xyz="0 -0.1 0" />
            <geometry>
                <box size="0.03 0.01 0.3" />
            </geometry>
            <material name="Black">
