            </geometry>
        </collision>
        <visual>
            <origin rpy="${pi / 2} 0 0" xyz="0 0.0525 0" />
            <geometry>
                <cylinder length="0.105" radius="0.05"/>
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>

        <collision>
            <origin rpy="${pi} 0 0" xyz="0 0.1 0" />
            <geometry>
                <box size="0.03 0.01 0.3" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="${pi} 0 0" xyz="0 0.1 0" />
            <geometry>
                <box size="0.03 0.01 0.3" />
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>

    </link>

